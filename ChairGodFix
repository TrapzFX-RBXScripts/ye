function effect02(CFrameMovement,Parent,Part,Brick_Color,TypeENUM,Tween_Brick_Color,Transparency,Tween_Transparency)
	local ts = game:GetService("TweenService")
	local goingto = true 

	--autofills some select variables

	if CFrameMovement == nil then
		CFrameMovement = true
	end
	--actual coding
	local mas = Parent
	local Part0 = Instance.new("SpawnLocation")
	local SpecialMesh1 = Instance.new("SpecialMesh")
	pcall(function()
		Part0.Decal.Parent = game.Lighting
	end)

	spawn(function()
		while true do
			wait(0.009)
			if goingto == true and CFrameMovement == true then
				local inf3 = TweenInfo.new(
					0.7,
					Enum.EasingStyle.Sine,
					Enum.EasingDirection.Out
				)

				local goals3 = {
					CFrame = Part.CFrame
				}

				local tween3 = ts:Create(Part0,inf3,goals3)
				tween3:Play()
			elseif goingto == false and CFrameMovement == false then
				break
			end
		end
	end)
	Part0.TeamColor = BrickColor.new("Really red")
	Part0.Parent = mas
	Part0.CanCollide = false
	Part0.CFrame = CFrame.new(-136.899994, 0.500003994, -3.70000005, 1, 0, 0, 0, 1, 0, 0, 0, 1)
	Part0.Position = Part.Position
	Part0.Orientation = Vector3.new(math.random(-360,360),math.random(-360,360),math.random(-360,360))
	Part0.BrickColor = Brick_Color
	Part0.Size = Vector3.new(4, 0.39999997615814, 0.19999992847443)
	Part0.BottomSurface = Enum.SurfaceType.Smooth
	Part0.Material = Enum.Material.Neon
	Part0.TopSurface = Enum.SurfaceType.Smooth
	Part0.Anchored = true
	SpecialMesh1.Parent = Part0
	SpecialMesh1.MeshType = TypeENUM

	local inf = TweenInfo.new(
		1,
		Enum.EasingStyle.Sine,
		Enum.EasingDirection.Out
	)

	local goals = {
		Size = Vector3.new(0.008, 0.39999997615814, 0.19999992847443),
		Color = Tween_Brick_Color
	}

	local tween = ts:Create(Part0,inf,goals)
	tween:Play()
	spawn(function()
		wait(.6)
		local inf2 = TweenInfo.new(
			0.4,
			Enum.EasingStyle.Sine,
			Enum.EasingDirection.Out
		)

		local goals2 = {
			Transparency = Transparency
		}

		local tween2 = ts:Create(Part0,inf2,goals2)
		tween2:Play()
	end)
	spawn(function()
		wait(1)
		goingto = false
		Part0.Parent = game.Lighting
	end)
end

NLS([[
local plr = game.Players.LocalPlayer
 
local mouse = plr:GetMouse()
 
local mousePos = mouse.Hit.p
 
local exploEvent = owner.Character.Event01
 
local exploEvent2 = owner.Character.Event02
 
mouse.KeyDown:Connect(function(k)
    if k == "e" then
       exploEvent:FireServer(mousePos)
    end
 
    if k == "r" then
       exploEvent2:FireServer()
    end
end)
]],owner.PlayerGui)
 
local sound2  = Instance.new("Sound",Part677)
sound2.SoundId = "rbxassetid://642890855"
sound2.Volume = 2
local rem = Instance.new("RemoteEvent", owner.Character)
local debounce = false
rem.Name = 'SKID'
 
NLS("local rem = owner.Character.SKID local mouse = game:GetService'Players'.LocalPlayer:GetMouse() mouse.Button1Down:Connect(function() rem:FireServer(mouse.Hit.Position, mouse.Target) end)", owner.PlayerGui)
 
 
rem.OnServerEvent:Connect(function(plr, pos, target)
	if debounce == false then
			local MAGNITUDE = (Part677.CFrame * CFrame.new(0,1,7).Position - pos).Magnitude
 
			if MAGNITUDE < 1000 then
                --[[
                				game:GetService'RunService'.Heartbeat:Connect(function()
					if Part6773.Transparency <= 0 then
						Part6773.Transparency = Part6773.Transparency + .05
					end
					if Part6773.Transparency == 1 then
						Part6773.Transparency = 1
					end
				end)
                ]]--
 
				local Shot = Instance.new("Part", script)
				Shot.Anchored = true
				spawn(function()
					while wait() do
						Shot.Color = Part677.Color
						Swait()
					end
				end)
				Shot.Material = 'Neon'
				Shot.Shape = 'Block'
				Shot.CFrame = CFrame.new(Part677.Position, pos) * CFrame.Angles(0,math.rad(90),0)
				Shot.Position = Shot.Position:Lerp(pos, .5)
				Shot.Name = 'SKIDSHOT'
				sound2:Play()
				Shot.CanCollide = false
				Shot.Size = Vector3.new(MAGNITUDE,.5,.5)
				game:GetService('Debris'):AddItem(Shot, 1)
 
 
				game:GetService'RunService'.Heartbeat:Connect(function()
					Shot.Size = Shot.Size + Vector3.new(0,.05,.05)
					Shot.Transparency = Shot.Transparency + .05
				end)
 
 
 
				if target.Parent:FindFirstChildWhichIsA("Humanoid") ~= nil and target.Name ~= 'SKIDSHOT' then
					for i,v in pairs(target.Parent:GetDescendants()) do
						if v.ClassName == 'Part' or v.ClassName == 'MeshPart' then
							game:GetService'Debris':AddItem(v.Parent, 3)
							v.Anchored = true
							v.CanCollide = false
							v.Parent = script
							game:GetService("TweenService"):Create(v, TweenInfo.new(3), {CFrame = CFrame.new(v.Position + Vector3.new(0,5,5)), Transparency = 1}):Play()
 
						end
					end
				end
				wait(0.5)
				rarmdowntween:Play()
				wait(1)
				debounce = false
			end
		end
end)
 
