spawn(function()
	Part8.CFrame = owner.Character.Torso.CFrame
	local weld = Instance.new("Weld")
	weld.Parent = Part8
	weld.Part0 = weld.Parent
	weld.Part1 = owner.Character.Torso
	weld.C0 = owner.Character.Torso.CFrame:ToObjectSpace(Part8.CFrame)
end)
 
local defaults = {
	LeftHip = owner.Character.Torso["Left Hip"].C0,
	RightHip = owner.Character.Torso["Right Hip"].C0,
	LeftShoulder = owner.Character.Torso["Left Shoulder"].C0,
	RightShoulder = owner.Character.Torso["Right Shoulder"].C0,
	Neck = owner.Character.Torso["Neck"].C0,
	RootJoint = owner.Character.HumanoidRootPart["RootJoint"].C0
}
 
local current = "Idle"
 
owner.Character.Animate:Destroy()
wait()
spawn(function()
	while true do
		wait(0.005)
		if current == "Idle" then
			local weld6 = owner.Character.HumanoidRootPart["RootJoint"]
			weld6.C0 = defaults.RootJoint*CFrame.Angles(0, 0, math.rad(15.011))
			local weld5 = owner.Character.Torso["Left Hip"]
			weld5.C0 = defaults.LeftHip*CFrame.new(-0.9, 0.8, 0) * CFrame.Angles(math.rad(2.578), math.rad(-0.229), math.rad(-14.668))
			local weld4 = owner.Character.Torso["Right Shoulder"]
			weld4.C0 = defaults.RightShoulder*CFrame.new(0.4, 0.5, 0.2) * CFrame.Angles(math.rad(17.189), math.rad(-28.934), math.rad(-171.715))
			local weld3 = owner.Character.Torso["Left Shoulder"]
			weld3.C0 = defaults.LeftShoulder*CFrame.new(-0.3, 0.3, 0) * CFrame.Angles(math.rad(90.012), math.rad(-50.019), 0)
			local weld2 = owner.Character.Torso["Right Hip"]
			weld2.C0 = defaults.RightHip*CFrame.new(0.8, 0.9, 0) * CFrame.Angles(math.rad(18.621), math.rad(1.604), math.rad(24.58))
			local weld = owner.Character.Torso.Neck
			weld.C0 = defaults.Neck*CFrame.Angles(math.rad(4.813), math.rad(-1.318), math.rad(-14.954))
		end
 
		if current ~= "Idle" then
			local weld6 = owner.Character.HumanoidRootPart["RootJoint"]
			weld6.C0 = defaults.RootJoint*CFrame.Angles(math.rad(24.981), 0, 0)
			local weld5 = owner.Character.Torso["Left Hip"]
			local weld5 = owner.Character.Torso["Left Hip"]
			weld5.C0 = defaults.LeftHip*CFrame.new(-0.9, 0.8, 0) * CFrame.Angles(math.rad(2.578), math.rad(-0.229), math.rad(-14.668))
			local weld4 = owner.Character.Torso["Right Shoulder"]
			weld4.C0 = defaults.RightShoulder*CFrame.new(0.4, 0.5, 0.2) * CFrame.Angles(math.rad(17.189), math.rad(-28.934), math.rad(-171.715))
			local weld3 = owner.Character.Torso["Left Shoulder"]
			weld3.C0 = defaults.LeftShoulder*CFrame.new(-0.3, 0.3, 0) * CFrame.Angles(math.rad(90.012), math.rad(-50.019), 0)
			local weld2 = owner.Character.Torso["Right Hip"]
			weld2.C0 = defaults.RightHip*CFrame.new(0.8, 0.9, 0) * CFrame.Angles(math.rad(18.621), math.rad(1.604), math.rad(24.58))
			local weld = owner.Character.Torso.Neck
			weld.C0 = defaults.Neck*CFrame.Angles(math.rad(4.813), math.rad(-1.318), math.rad(-14.954))
		end
	end
end)
--[[
for i,charmesh in pairs(owner.Character:GetChildren()) do
	if charmesh:IsA("Part") then
		charmesh.BrickColor = BrickColor.new("Medium stone grey")
	end
	if charmesh:IsA("Accessory") then
		charmesh:Destroy()
	end
	if charmesh:IsA("CharacterMesh") then
		charmesh:Destroy()
	end
end
]]--
function iswalking(speed)
	if speed > 1 then
		current = "Walking"
	else
		current = "Idle"
	end
end
 
 
 
owner.Character.Humanoid.Running:Connect(iswalking)
 
local remote01 = Instance.new("RemoteEvent")
remote01.Name = "Event01"
remote01.Parent = owner.Character
 
local remote02 = Instance.new("RemoteEvent")
remote02.Name = "Event02"
remote02.Parent = owner.Character
 
