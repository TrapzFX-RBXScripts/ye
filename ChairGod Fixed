	while true do
		wait(.01)
		Part75.BrickColor = BrickColor.Random()
		for i,v in pairs(Model0:GetChildren()) do
			if v.Name == "NeonParts" or v.Name == "NeonPart" then
				v.BrickColor = Part75.BrickColor
			end
