local defaults = {
	LeftHip = owner.Character.Torso["Left Hip"].C0,
	RightHip = owner.Character.Torso["Right Hip"].C0,
	LeftShoulder = owner.Character.Torso["Left Shoulder"].C0,
	RightShoulder = owner.Character.Torso["Right Shoulder"].C0,
	Neck = owner.Character.Torso["Neck"].C0,
	RootJoint = owner.Character.HumanoidRootPart["RootJoint"].C0
}

local current = "Idle"

owner.Character.Animate:Destroy()

spawn(function()
	while true do
		wait(0.005)
		if current == "Idle" then
			local weld6 = owner.Character.HumanoidRootPart["RootJoint"]
			weld6.C0 = defaults.RootJoint*CFrame.new(0, 0, 3.211) * CFrame.Angles(math.rad(-10.027), 0, 0)
			local weld5 = owner.Character.Torso["Left Hip"]
			weld5.C0 = defaults.LeftHip*CFrame.new(0.013, 0.02, 0) * CFrame.Angles(0, 0, math.rad(10.084))
			local weld4 = owner.Character.Torso["Right Shoulder"]
			weld4.C0 = defaults.RightShoulder*CFrame.Angles(math.rad(90.012), math.rad(64.973), math.rad(4.927))
			local weld3 = owner.Character.Torso["Left Shoulder"]
			weld3.C0 = defaults.LeftShoulder*CFrame.new(-0.363, 0.199, 0) * CFrame.Angles(math.rad(89.21), math.rad(-32.487), math.rad(-0.229))
			local weld2 = owner.Character.Torso["Right Hip"]
			weld2.C0 = defaults.RightHip*CFrame.new(0.609, 0.395, 0) * CFrame.Angles(0, 0, math.rad(-10.141))
			local weld = owner.Character.Torso.Neck
			weld.C0 = defaults.Neck*CFrame.new(0, 0, 0.023) * CFrame.Angles(math.rad(10.027), 0, 0)
		end

		if current ~= "Idle" then
			local weld6 = owner.Character.HumanoidRootPart["RootJoint"]
			weld6.C0 = defaults.RootJoint*CFrame.new(0, 0, 3.211) * CFrame.Angles(math.rad(44.977), 0, 0)
			local weld5 = owner.Character.Torso["Left Hip"]
			weld5.C0 = defaults.LeftHip*CFrame.Angles(math.rad(-0.057), math.rad(-0.057), math.rad(50.019))
			local weld4 = owner.Character.Torso["Right Shoulder"]
			weld4.C0 = defaults.RightShoulder*CFrame.new(0.035, -0.035, 0) * CFrame.Angles(math.rad(-0.057), math.rad(0.057), math.rad(-50.019))
			local weld3 = owner.Character.Torso["Left Shoulder"]
			weld3.C0 = defaults.LeftShoulder*CFrame.new(-0.102, -0.102, 0) * CFrame.Angles(math.rad(-0.057), math.rad(-0.057), math.rad(45.092))
			local weld2 = owner.Character.Torso["Right Hip"]
			weld2.C0 = defaults.RightHip*CFrame.new(0.699, 0.699, 0) * CFrame.Angles(math.rad(-0.057), 0, math.rad(-35.008))
			local weld = owner.Character.Torso.Neck
			weld.C0 = defaults.Neck*CFrame.new(0, -0.001, 0.001) * CFrame.Angles(math.rad(-39.992), 0, 0)
		end
	end
end)

for i,charmesh in pairs(owner.Character:GetChildren()) do
	if charmesh:IsA("CharacterMesh") then
		charmesh:Destroy()
	end
end

function iswalking(speed)
	if speed > 1 then
		current = "Walking"
	else
		current = "Idle"
	end
end

owner.Character.Humanoid.Running:Connect(iswalking)

local sound = Instance.new("Sound")
sound.Parent = Part1
sound.SoundId = "rbxassetid://4865063065"
sound.Volume = 0.2
sound.RollOffMaxDistance = 50
sound.RollOffMinDistance = 50
sound.Looped = true
sound:Play()

owner.Character.Humanoid.WalkSpeed = 50

local remote01 = Instance.new("RemoteEvent")
remote01.Name = "Event01"
remote01.Parent = owner.Character

local remote02 = Instance.new("RemoteEvent")
remote02.Name = "Event02"
remote02.Parent = owner.Character


NLS([[
local plr = game.Players.LocalPlayer

local mouse = plr:GetMouse()

local mousePos = mouse.Hit.p

local exploEvent = owner.Character.Event01

local exploEvent2 = owner.Character.Event02

mouse.KeyDown:Connect(function(k)
    if k == "e" then
       exploEvent:FireServer(mousePos)
    end
    
    if k == "r" then
       exploEvent2:FireServer()
    end
end)
]],owner.PlayerGui)

local sound  = Instance.new("Sound",owner.Character.Head)
sound.SoundId = "rbxassetid://642890855"
sound.Volume = 2
local rem = Instance.new("RemoteEvent", owner.Character)
local debounce = false
rem.Name = 'SKID'

NLS("local rem = owner.Character.SKID local mouse = game:GetService'Players'.LocalPlayer:GetMouse() mouse.Button1Down:Connect(function() rem:FireServer(mouse.Hit.Position, mouse.Target) end)", owner.PlayerGui)


rem.OnServerEvent:Connect(function(plr, pos, target)
	if debounce == false then
			local MAGNITUDE = (Part6773.CFrame * CFrame.new(0,1,7).Position - pos).Magnitude

			if MAGNITUDE < 1000 then
                --[[
                				game:GetService'RunService'.Heartbeat:Connect(function()
					if Part6773.Transparency <= 0 then
						Part6773.Transparency = Part6773.Transparency + .05
					end
					if Part6773.Transparency == 1 then
						Part6773.Transparency = 1
					end
				end)
                ]]--

				local Shot = Instance.new("Part", script)
				Shot.Anchored = true
				spawn(function()
					while wait() do
						Shot.Color = Part6773.Color
						Swait()
					end
				end)
				Shot.Material = 'Neon'
				Shot.Shape = 'Block'
				Shot.CFrame = CFrame.new(Part6773.Position, pos) * CFrame.Angles(0,math.rad(90),0)
				Shot.Position = Shot.Position:Lerp(pos, .5)
				Shot.Name = 'SKIDSHOT'
				local RS = owner.Character.Torso["Right Shoulder"]
				local tweenservice = game:GetService("TweenService")
				local rarmuptween = tweenservice:Create(RS,TweenInfo.new(0.3,Enum.EasingStyle.Sine,Enum.EasingDirection.Out),{C0 = RS.C0 * CFrame.Angles(math.rad(0), math.rad(0), math.rad(100))})
				local rarmdowntween = tweenservice:Create(RS,TweenInfo.new(0.5,Enum.EasingStyle.Back,Enum.EasingDirection.Out),{C0 = RS.C0 * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0))})
				rarmuptween:Play()
				sound:Play()
				Shot.CanCollide = false
				Shot.Size = Vector3.new(MAGNITUDE,.5,.5)
				game:GetService('Debris'):AddItem(Shot, 1)


				game:GetService'RunService'.Heartbeat:Connect(function()
					Shot.Size = Shot.Size + Vector3.new(0,.05,.05)
					Shot.Transparency = Shot.Transparency + .05
				end)



				if target.Parent:FindFirstChildWhichIsA("Humanoid") ~= nil and target.Name ~= 'SKIDSHOT' then
					for i,v in pairs(target.Parent:GetDescendants()) do
						if v.ClassName == 'Part' or v.ClassName == 'MeshPart' then
							game:GetService'Debris':AddItem(v.Parent, 3)
							v.Anchored = true
							v.CanCollide = false
							v.Parent = script
							game:GetService("TweenService"):Create(v, TweenInfo.new(3), {CFrame = CFrame.new(v.Position + Vector3.new(0,5,5)), Transparency = 1}):Play()

						end
					end
				end
				wait(0.5)
				rarmdowntween:Play()
				wait(1)
				debounce = false
			end
		end
end)

spawn(function()
	while true do
		task.wait()
		effect02(true,owner.Character,Part6773,BrickColor.new("Bright blue"),Enum.MeshType.Sphere,BrickColor.new("Really black"),0.3,1)
	end
end)

