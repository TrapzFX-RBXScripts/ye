spawn(function()
	Part6773.CFrame = owner.Character["Right Arm"].CFrame
	local weld = Instance.new("Weld")
	weld.Parent = Part6773
	weld.Part0 = weld.Parent
	weld.Part1 = owner.Character["Right Arm"]
	weld.C0 = owner.Character["Right Arm"].CFrame:ToObjectSpace(Part6773.CFrame)
end)

--functions from EzEffects
function effect02(CFrameMovement,Parent,Part,Brick_Color,TypeENUM,Tween_Brick_Color,Transparency,Tween_Transparency)
	local ts = game:GetService("TweenService")
	local goingto = true 

	--autofills some select variables

	if CFrameMovement == nil then
		CFrameMovement = true
	end
	--actual coding
	local mas = Parent
	local Part0 = Instance.new("SpawnLocation")
	local SpecialMesh1 = Instance.new("SpecialMesh")
	pcall(function()
		Part0.Decal.Parent = game.Lighting
	end)

	spawn(function()
		while true do
			wait(0.009)
			if goingto == true and CFrameMovement == true then
				local inf3 = TweenInfo.new(
					0.7,
					Enum.EasingStyle.Sine,
					Enum.EasingDirection.Out
				)

				local goals3 = {
					CFrame = Part.CFrame
				}

				local tween3 = ts:Create(Part0,inf3,goals3)
				tween3:Play()
			elseif goingto == false and CFrameMovement == false then
				break
			end
		end
	end)
	Part0.TeamColor = BrickColor.new("Really red")
	Part0.Parent = mas
	Part0.CanCollide = false
	Part0.CFrame = CFrame.new(-136.899994, 0.500003994, -3.70000005, 1, 0, 0, 0, 1, 0, 0, 0, 1)
	Part0.Position = Part.Position
	Part0.Orientation = Vector3.new(math.random(-360,360),math.random(-360,360),math.random(-360,360))
	Part0.BrickColor = Brick_Color
	Part0.Size = Vector3.new(4, 0.39999997615814, 0.19999992847443)
	Part0.BottomSurface = Enum.SurfaceType.Smooth
	Part0.Material = Enum.Material.Neon
	Part0.TopSurface = Enum.SurfaceType.Smooth
	spawn(function()
		Part0.CFrame = Part.CFrame
		local weld = Instance.new("Weld")
		weld.Parent = Part0
		weld.Part0 = Part0
		weld.Part1 = Part
		weld.C0 = Part.CFrame:ToObjectSpace(Part0.CFrame)
	end)
	SpecialMesh1.Parent = Part0
	SpecialMesh1.MeshType = TypeENUM

	local inf = TweenInfo.new(
		1,
		Enum.EasingStyle.Sine,
		Enum.EasingDirection.Out
	)

	local goals = {
		Size = Vector3.new(0.008, 0.39999997615814, 0.19999992847443),
		BrickColor = Tween_Brick_Color
	}

	local tween = ts:Create(Part0,inf,goals)
	tween:Play()
	spawn(function()
		wait(.6)
		local inf2 = TweenInfo.new(
			0.4,
			Enum.EasingStyle.Sine,
			Enum.EasingDirection.Out
		)

		local goals2 = {
			Transparency = Transparency
		}

		local tween2 = ts:Create(Part0,inf2,goals2)
		tween2:Play()
	end)
	spawn(function()
		wait(1)
		goingto = false
		Part0.Parent = game.Lighting
	end)
end

